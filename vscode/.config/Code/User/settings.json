{
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "window.restoreFullscreen": true,
    "explorer.confirmDelete": false,
    "editor.tabSize": 4,
    "workbench.list.openMode": "singleClick",
    "editor.largeFileOptimizations": false,
    "python.pythonPath": "/Users/sspaeti/.venvs/onenote-export/bin/python",
    "kite.showWelcomeNotificationOnStartup": false,
    "[dockerfile]": {
        "editor.formatOnSave": false,
    },
    "autoDocstring.quoteStyle": "'''",
    "autoDocstring.docstringFormat": "google",
    "breadcrumbs.enabled": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveTimeout": 2000,
    "editor.formatOnPaste": false,
    "editor.rulers": [
        100,
    ],
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.suggestSelection": "first",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],
    "files.autoSave": "off",
    "files.exclude": {
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/.DS_Store": true,
        "**/.cache": true,
        "**/.files": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.idea": true,
        "**/.ipynb_checkpoints": true,
        "**/.mypy_cache": true,
        "**/.offline-package-cache": true,
        "**/.pytest_cache": true,
        "**/.ropeproject": true,
        "**/.runs": true,
        "**/.svn": true,
        "**/.temp": true,
        "**/.tox": true,
        "**/.vscode": true,
        "**/CVS": true,
        "**/__pycache__": true,
        "**/access.log": true,
        "**/build": true,
        "**/error.log": true,
        "**/node_modules": true,
        "**/settings_comments.txt": true,
        "**/spark-warehouse": true,
        "**/tags": true,
        "*/.coverage": true,
        "*/coverage.xml": true,
        "*/dist": true,
        "*/htmlcov": true,
        "pulumi/bin": true,
        "scala_modules/*/target": true,
    },
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "git.ignoreLimitWarning": true,
    "python.analysis.disabled": [
        "unresolved-import",
    ],
    "python.analysis.memory.keepLibraryAst": true,
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length",
        "88",
        "-S",
        "--fast",
    ],
    "python.languageServer": "Jedi",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.lintOnSave": true,
    "python.linting.pycodestyleEnabled": false,
    "python.linting.pylintArgs": [
        "--rcfile",
        "~/.pylintrc",
    ],
    "python.testing.pytestEnabled": true,
    "python.venvPath": "/Users/sspaeti/.venvs/",
    "workbench.colorTheme": "Kanagawa",
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.tree.renderIndentGuides": "onHover",
    "[typescript]": {
        "editor.tabSize": 2,
    },
    "extensions.autoUpdate": true,
    "explorer.confirmDragAndDrop": false,
    "git.autofetch": true,
    "editor.renderLineHighlight": "none",
    "editor.overviewRulerBorder": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.folding": true,
    "editor.occurrencesHighlight": "off",
    "editor.matchBrackets": "never",
    "editor.glyphMargin": true,
    "explorer.openEditors.visible": 0,
    "workbench.editor.showIcons": false,
    "workbench.editor.tabCloseButton": "off",
    "indenticator.color.dark": "rgba(255,255,255,.1)",
    "workbench.colorCustomizations": {
        "tab.activeBorder": "#0000",
    },
    "editor.wordBasedSuggestions": "matchingDocuments",
    "gitHistory.showEditorTitleMenuBarIcons": false,
    "gitlens.menus": {
        editor: {
            blame: false,
            clipboard: true,
            compare: true,
            details: false,
            history: false,
            remote: false,
        },
        editorGroup: false,
        editorTab: false,
        explorer: {
            clipboard: true,
            compare: true,
            history: true,
            remote: true,
        },
        scmGroup: {
            compare: true,
            openClose: true,
            stash: true,
            stashInline: true,
        },
        scmItem: {
            clipboard: true,
            compare: true,
            history: true,
            remote: true,
            stash: true,
        },
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
    },
    "jupyter.alwaysTrustNotebooks": true,
    "outline.showVariables": false,
    "[html]": {
        "editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter",
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
    },
    "mssql.connections": [
        {
            server: "{{put-server-name-here}}",
            database: "{{put-database-name-here}}",
            user: "{{put-username-here}}",
            password: "",
        },
    ],
    "grammarly.dialect": "british",
    "[sql]": {
        "editor.defaultFormatter": "dorzey.vscode-sqlfluff",
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left",
    },
    "python.defaultInterpreterPath": "/Users/sspaeti/.venvs/onenote-export/bin/python",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            before: [
                "j",
                "k",
            ],
            after: [
                "<Esc>",
            ],
        },
    ],
    "vim.visualModeKeyBindings": [
        {
            before: [
                "<leader>",
                "c",
            ],
            commands: [
                "editor.action.commentLine",
            ],
            when: "editorTextFocus && !editorReadonly",
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            before: [
                "<leader>",
                "d",
            ],
            after: [
                "d",
                "d",
            ],
        },
        {
            before: [
                "<C-n>",
            ],
            commands: [
                ":nohl",
            ],
        },
        {
            before: [
                "s",
                "l",
            ],
            after: [],
            commands: [
                "workbench.action.focusRightGroup",
            ],
        },
        {
            before: [
                "s",
                "h",
            ],
            after: [],
            commands: [
                "workbench.action.focusLeftGroup",
            ],
        },
        {
            before: [
                "s",
                "s",
            ],
            after: [],
            commands: [
                "workbench.action.splitEditorDown",
            ],
        },
        {
            before: [
                "s",
                "v",
            ],
            after: [],
            commands: [
                "workbench.action.splitEditorRight",
            ],
        },
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
    },
    "editor.lineNumbers": "relative",
    "redhat.telemetry.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "github.copilot.enable": {
        "*": true,
        plaintext: true,
        markdown: false,
        scminput: false,
        yaml: true,
        lua: true,
    },
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true,
    },
    "telemetry.telemetryLevel": "off",
    "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/sspaeti/.config/nvim/",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
    },
    "codegpt.model": "ChatGPT",
    "workbench.editor.tabActionCloseVisibility": false,
    "mssql.objectExplorer.groupBySchema": true,
    "grammarly.files.include": [
        "**/readme.md",
        "**/README.md",
        "**/*.txt",
        "*.md",
    ],
    "github.copilot.editor.enableAutoCompletions": true,
    "git.ignoreMissingGitWarning": true,
}