  "custom/cpu-governor": {
    "format": " {}", 
    "exec": "powerprofilesctl get | case $(cat) in 'performance') echo '‚ö°';; 'power-saver') echo '‚ñí';; 'balanced') echo '‚öñÔ∏è';; *) echo '‚ùì';; esac",
    "exec-if": "command -v powerprofilesctl",
    "interval": 5,
